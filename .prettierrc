{
	"trailingComma": "all",
	"useTabs": true,
	"tabWidth": 2,
	"printWidth": 110,
	"semi": true,
	"singleQuote": true,
	"bracketSpacing": true,
	"plugins": ["prettier-plugin-css-order", "prettier-plugin-astro"],
	"overrides": [
		{
			"files": ["tsconfig.json", "jsconfig.json"],
			"options": {
				"parser": "jsonc"
			}
		}
	]
}
